{
"name": "tos-thinkscript",
"displayName": "thinkscript",
"description": "Syntax highlighting for the thinkScript language for thinkorswim trading platform",
"version": "1.0.0",
"publisher": "Darth Buddha",
"homepage": "https://github.com/DarthBuddha/vsc-ThinkScript-Language",
"icon": "images/tos_logo.jpg",
"repository": {
    "type": "git",
    "url": "https://github.com/DarthBuddha/vsc-ThinkScript-Language.git"
},
"engines": {
    "vscode": "^1.82.0"
},
"categories": [
    "Programming Languages"
],
"contributes": {
    "languages": [
            {
            "id": "thinkscript",
            "aliases": [
                    "thinkScript",
                    "thinkscript",
                    "ThinkScript"
            ],
            "extensions": [
                    ".ts"
            ],
            "configuration": "./language-configuration.json"
            }
    ],
    "grammars": [
            {
            "language": "thinkscript",
            "scopeName": "source.ts",
            "path": "./syntaxes/thinkscript.tmLanguage.json"
            }
    ]
},
"contributors": [
    {
    "name" : "Darth Buddha",
    "url" : "https://github.com/DarthBuddha/vsc-ThinkScript-Language"
    }
]
}
